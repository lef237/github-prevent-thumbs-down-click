{
  "manifest_version": 3,
  "name": "GitHub Thumbs Down Remover",
  "version": "1.0",
  "description": "Removes the Thumbs Down reaction from GitHub",
  "host_permissions": ["https://github.com/*"],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ]
}
